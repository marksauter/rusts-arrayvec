{
  "extends": [
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-empty": false,
    "no-unused-variable": [true, {"ignore-pattern": "^_"}],
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case",
        "allow-snake-case"
      ]
    }
  }
}
